{% extends 'base.html.twig' %}

{% block title %}HomePage des quizz{% endblock %}

{% block body %}

<div class="quizz-container">

{% for quizz in quizzes %}

<div class="quizz-card">
<a href="{{path('quizz',{id:quizz.id})}}" class="quizz-button">
 <img src="{{ asset(quizz.imgUrl) }}" />
</a>
<p>{{quizz.title}}</p>
</div>


{% endfor %}
</div>

{% endblock %}
